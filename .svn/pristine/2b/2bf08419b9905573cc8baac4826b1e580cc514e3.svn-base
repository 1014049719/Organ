<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/iv_FrameLayout_browser"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/activity_bg">

    <ImageView
        android:id="@+id/iv_pic_browser"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_margin="@dimen/space_3_0"
        android:scaleType="centerCrop"
        android:src="@mipmap/load_image_200" />

    <ImageView
        android:id="@+id/cb_pic_browser"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_gravity="right|bottom"
        android:checked="true"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="center"
        android:layout_marginRight="@dimen/space_2_0"
        android:src="@mipmap/checkbox_album_img_unselected" />

    <ImageView
        android:id="@+id/cb_pic_already_select"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|bottom"
        android:gravity="center"
        android:src="@mipmap/photo_ysc"
        android:visibility="gone" />

</FrameLayout>

public class ModifyCircleEvent {
    public long circleId;
    public String description;
    public String photo;
    public String cover;

    public ModifyCircleEvent(long circleId, String description, String photo, String cover){
        this.circleId = circleId;
        this.description = description;
        this.photo = photo;
        this.cover = cover;
    }
}
